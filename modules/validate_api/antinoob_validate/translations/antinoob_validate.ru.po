#: antinoob_validate.module:11
msgid "Exclamation"
msgstr "Восклицательные знаки"

#: antinoob_validate.module:12
msgid "Disallow to use multiple exclamation symbols in a row."
msgstr "Запрет на использование нескольки восклицательных знаков подряд"

#: antinoob_validate.module:19
msgid "Caps Lock"
msgstr "Заглавные буквы"

#: antinoob_validate.module:20
msgid "Disallow to use all capital words."
msgstr "Запрет на использование только заглавных букв в тексе"

#: antinoob_validate.module:29
msgid "Disallowed words"
msgstr "Запрещенные слова"

#: antinoob_validate.module:30
msgid "Disallow to use word(s): !words."
msgstr "Запрет на использование слов: !words."

#: antinoob_validate.module:62
msgid "[@word] is forbidden to use."
msgstr "Запрещено употреблять: @word"

#: antinoob_validate.module:92
msgid "Do not use more than one exclamation point in a row."
msgstr "Не используйте более чем 1 восклицательный знак подряд."

#: antinoob_validate.module:124
msgid "Using ALL capital symbols is forbidden. "
msgstr "Запрещено писать текст только заглавными буквами"

#: antinoob_validate.info:0
msgid "Antinoob Validate"
msgstr "Антинубская валидация текста"

#: antinoob_validate.info:0
msgid "Provide few antinoob validators."
msgstr "Предоставляет несколько антинубских валидаций текста."
